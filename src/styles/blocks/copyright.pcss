.copyright {
  font-size: 12px;
  line-height: 34px;
  color: #7a7a7a;
}

.copyright__list {
  display: flex;
}

.copyright__item {
  display: flex;
  align-items: center;
  &:last-child {
    margin-right: 0;
  }
}

.copyright__item:after {
  display: block;
  content: '';
  width: 1px;
  height: 10px;
  background-color: #7a7a7a;
  margin-left: 20px;
  margin-right: 20px;
}

.copyright__item:last-child:after {
  display: none;
}