.compare-button {
  display: flex;
  position: relative;
  font-size: 12px;
  color: #AAAAAA;
  background-color: transparent;
  align-items: center;
  padding-left: 25px;
  margin-bottom: 20px;
}

.compare-button__icon {
  position: absolute;
  width: 15px;
  height: 15px;
  left: 0;
  fill: #AAAAAA;
}